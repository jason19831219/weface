<view class='tab-bar'>

  <view class='tab-item' bindtap='tabLeftTap'>
    <view class="item-holder left {{iconSelected=='left'?'selected':''}}">
      <span class="iconfont icon-face"></span>
      <text>脸卡</text>
    </view>
  </view>

  <view class='tab-item' bindtap='tabRightTap'>
    <view class="item-holder right {{iconSelected=='right'?'selected':''}}">
      <span class="iconfont icon-collection"></span>
      <text>收藏</text>
    </view>
  </view>
</view>